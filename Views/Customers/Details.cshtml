@model Vidly.Models.Customer

@{
    ViewData["Title"] = "Customer Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Customer Detail</h1>
<table class = "table table-hover">
    <thead>
        <th>Name</th>
        <th>Birthdate</th>
    </thead>
    <tr>
        <td>@Model.Name</td>
        <td>@Model.BirthDate</td>
    </tr>
</table>